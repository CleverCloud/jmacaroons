<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='jmacaroons'>

  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

  <inherits name="com.googlecode.cryptogwt.JceEmulation"/>

  <inherits name="com.neilalexander.jnacl.crypto.jnacl"/>

  <inherits name='org.timepedia.exporter.Exporter'/>
  <set-property name="export" value="yes"/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.github.nitram509.jmacaroons.gwt.MacaroonModuleEntryPoint'/>

  <!-- Specify the paths for translatable code                    -->
  <source path=''/>
  <source path='util'/>
  <source path='verifier'/>

</module>